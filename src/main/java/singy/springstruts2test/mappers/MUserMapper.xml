<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="singy.springstruts2test.mappers.MUserMapper">
  <select id="selectAll" resultType="singy.springstruts2test.entities.MUser">
    select * from m_user
  </select>

  <select id="selectByCd" resultType="singy.springstruts2test.entities.MUser">
    select * from m_user where user_cd = #{cd}
  </select>
</mapper>